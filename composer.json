{
    "name": "still-code/signature",
    "description": "laravel packages to add @still-code and @zeus to the footer where it's needed in amm our projects",
    "keywords": [
        "laravel",
        "lara-zeus",
        "still-code"
    ],
    "homepage": "https://github.com/still-code",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ashraf Monshi",
            "email": "wh7r.com@gmail.com",
            "role": "Owner"
        }
    ],
    "require": {
        "php": "^8.0",
        "jenssegers/agent": "^2.6"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "StillCode\\Signature\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "StillCode\\Signature\\SignatureServiceProvider"
            ]
        }
    }
}
